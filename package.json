{
  "name": "react-native-draggable-flatlist",
  "version": "1.0.5",
  "description": "A drag-and-drop-enabled FlatList component for React Native",
  "main": "node_modules/expo/AppEntry.js",
  "module": "lib/module/index",
  "react-native": "src/index.tsx",
  "types": "lib/typescript/index.d.ts",
  "scripts": {
    "start": "npx expo run:android",
    "startWithoutRebuildApk": "npx expo start",
    "devOld": "npx expo start --dev-client",
    "ios": "npx expo run:ios",
    "web": "npx expo start --web",
    "eject": "npx expo eject",
    "test": "jest --watchAll",
    "typecheck": "tsc --skipLibCheck --noEmit",
    "build": "npx expo build:android -t app-bundle",
    "publishApk": "./buildApk.bat",
    "postinstall": "patch-package"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/computerjazz/react-native-draggable-flatlist.git"
  },
  "keywords": [
    "react-native",
    "sortable",
    "draggable",
    "flatlist",
    "drag",
    "drop",
    "sort",
    "list"
  ],
  "author": "Daniel Merrill",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/computerjazz/react-native-draggable-flatlist/issues"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "./node_modules/react-native-gesture-handler/jestSetup.js",
      "./jest-setup.js"
    ],
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    },
    "transformIgnorePatterns": []
  },
  "homepage": "https://github.com/computerjazz/react-native-draggable-flatlist#readme",
  "peerDependencies": {
    "react-native-gesture-handler": ">=2.0.0",
    "react-native-reanimated": ">=2.8.0"
  },
  "devDependencies": {
    "@testing-library/react-native": "^7.2.0",
    "@types/react": "~17.0.21",
    "@types/react-native": "^0.64.5",
    "babel-jest": "^26.6.3",
    "husky": "^4.2.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.71.0",
    "prettier": "^2.2.1",
    "pretty-quick": "^2.0.1",
    "react-native-builder-bob": "^0.18.2",
    "react-test-renderer": "^17.0.2",
    "typescript": "^4.2.4"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "commonjs",
        {
          "configFile": "./babel.config.js"
        }
      ],
      "module",
      "typescript"
    ]
  },
  "dependencies": {
    "@babel/preset-typescript": "^7.17.12",
    "@expo/vector-icons": "^13.0.0",
    "@gemedico/react-native-sounds": "^1.0.2",
    "@react-native-async-storage/async-storage": "^1.17.10",
    "@react-native-community/datetimepicker": "3.0.0",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-community/slider": "4.2.1",
    "@react-native-material/core": "1.3.5",
    "@react-navigation/bottom-tabs": "^6.5.2",
    "@react-navigation/native": "^6.0.13",
    "@react-navigation/native-stack": "^6.9.0",
    "@react-navigation/stack": "^6.3.2",
    "@reduxjs/toolkit": "^1.9.1",
    "axios": "^1.1.3",
    "expo": "~45.0.0",
    "expo-asset": "~8.5.0",
    "expo-constants": "~13.1.1",
    "expo-font": "~10.1.0",
    "expo-linking": "~3.1.0",
    "expo-splash-screen": "~0.15.1",
    "expo-sqlite": "~10.2.0",
    "expo-status-bar": "~1.3.0",
    "expo-system-ui": "~1.2.0",
    "expo-updates": "~0.13.4",
    "expo-web-browser": "~10.2.0",
    "install": "^0.13.0",
    "lodash.debounce": "^4.0.8",
    "moment": "^2.29.4",
    "patch-package": "^7.0.2",
    "postinstall-postinstall": "^2.1.0",
    "react": "18.1.0",
    "react-calendar": "^4.4.0",
    "react-dom": "17.0.2",
    "react-native": "0.68.2",
    "react-native-animatable": "^1.3.3",
    "react-native-chart-kit": "^6.12.0",
    "react-native-date-picker": "^4.2.5",
    "react-native-document-picker": "^9.0.1",
    "react-native-draggable-dynamic-flatlist": "^1.0.4",
